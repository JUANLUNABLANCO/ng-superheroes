{
  "name": "superheroes-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "ng:dev": "ng serve --open",
    "ng:dev:watch": "ng serve --open --watch --configuration development",
    "ng:build:dev": "ng build --configuration development",
    "ng:build:prod": "rimraf ./dist && ng build --configuration=production",
    "ng:serve:prod": "npx http-server -p 8080 --cors -c-1 dist",
    "ng:build-serve:prod": "rimraf ./dist && ng build --configuration=production && npx http-server -p 8080 -c-1 dist",
    "docker:ng:build": "docker build --no-cache --progress=plain -t ng-app-superheroes-image .",
    "docker:ng:run": "docker run -d -it -p 80:80/tcp ng-app-superheroes-image",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@auth0/angular-jwt": "^5.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "rimraf": "^5.0.5",
    "typescript": "~4.8.2"
  }
}
